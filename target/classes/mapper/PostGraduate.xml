<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo2.Dao.PostGraduateDao">

    <select id="selectAll"   resultType="com.example.demo2.Entity.PostGraduate">
            SELECT id, name FROM  postgraduate
    </select>
    <select id="selectId"   resultType="com.example.demo2.Entity.PostGraduate">
            SELECT * FROM  postgraduate where id = #{id}
    </select>

</mapper>